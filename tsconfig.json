{
  "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
      "types": [
        "node"
      ],
      "baseUrl": "./",
      "paths": {
        "@components/*": ["./src/components/*"],
        "@screens/*": ["./src/screens/*"],
        "@assets/*": ["./src/assets/*"],
        "@fonts/*": ["./src/theme/fonts"],
        "@validations": ["./src/validations"],
        "@navigatior": ["./src/navigatior"],
        "@helper": ["./src/helper"],
        "@context": ["./src/context"],
        "@images/*": ["./src/assets/images/*"],
        "@style" : ["./src/theme/index"]
      }
    }
}
